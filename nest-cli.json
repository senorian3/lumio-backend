{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/lumio/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/lumio/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/lumio",
  "projects": {
    "files": {
      "type": "application",
      "root": "apps/files",
      "entryFile": "app/main",
      "sourceRoot": "apps/files/src",
      "compilerOptions": {
        "tsConfigPath": "apps/files/tsconfig.app.json"
      }
    },
    "lumio": {
      "type": "application",
      "root": "apps/lumio",
      "entryFile": "app/main",
      "sourceRoot": "apps/lumio/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lumio/tsconfig.app.json"
      }
    },
    "payments": {
      "type": "application",
      "root": "apps/payments",
      "entryFile": "app/main",
      "sourceRoot": "apps/payments/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payments/tsconfig.app.json"
      }
    }
  }
}
